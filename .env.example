# Environment Configuration Template
# Copy this file to .env.development or .env.production and fill in the values

# --- Application ---
NODE_ENV=development
PORT=5000

# --- Admin Credentials ---
ADMIN_USER=your_admin_user
ADMIN_PASSWORD=your_admin_password

# --- Security ---
SECRET_KEY=your_secret_key_here

# --- Database ---
DB_USERNAME=genkun_user
DB_PASSWORD=your_db_password
DB_HOST=localhost
DB_PORT=5433
DB_NAME=postgres

# --- Database (Docker) ---
POSTGRES_DB=postgres
POSTGRES_USER=genkun_user
POSTGRES_PASSWORD=your_db_password

# --- PgAdmin ---
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=your_pgadmin_password

# --- Redis ---
REDIS_HOST=localhost
REDIS_PORT=6379

# --- AI API Keys ---
# Google STT (JSON file path or key)
GOOGLE_APPLICATION_CREDENTIALS=./google-key.json

# OpenAI (Analysis)
OPENAI_API_KEY=your_openai_api_key

# --- Build Info ---
BRANCH=DEV
SHORT_SHA=1
APP_VERSION=1.0.0
